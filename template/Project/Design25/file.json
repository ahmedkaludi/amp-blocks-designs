  "content": "<!-- wp:amp/rowlayout {\"uniqueID\":\"_a00\",\"columns\":1,\"colLayout\":\"equal\",\"maxWidth\":1100,\"topPadding\":50,\"bottomPadding\":50,\"bgColor\":\"#fcf5e8\",\"blockAlignment\":\"full\"} -->\n<div class=\"wp-block-amp-rowlayout cr alignfull\"><div id=\"ri_a00\" class=\"ab rb ri_a00\"><div class=\"cw ab1 gd gvd rvtop rlequal tlinherit rmc-left-to-right mrow\"><!-- wp:amp/column {\"uniqueID\":\"_625\"} -->\n<div class=\"wp-block-amp-column c c-1 cd_625\"><div class=\"ci\"><!-- wp:ampblocks/heading {\"level\":3,\"uniqueID\":\"_60a\",\"size\":40,\"typography\":\"Work Sans\",\"googleFont\":true,\"fontSubset\":\"latin\",\"fontVariant\":\"600\",\"fontWeight\":\"600\"} -->\n<h3 id=\"h_60a\" class=\"h_60a hp wp-block-amp-advancedheading\" style=\"margin-top:10px;margin-bottom:10px\">A&nbsp;life-changing experience</h3>\n<!-- /wp:ampblocks/heading -->\n\n<!-- wp:amp/advancedheading {\"uniqueID\":\"_cab\",\"size\":18,\"typography\":\"Work Sans\",\"googleFont\":true,\"fontSubset\":\"latin\",\"fontVariant\":\"regular\",\"fontWeight\":\"400\"} -->\n<p id=\"h_cab\" class=\"h_cab pp wp-block-amp-advancedheading\" style=\"margin-top:10px;margin-bottom:10px\">During our 9-week (full-time) or 24-week (part-time) coding Bootcamp, learn the most in-demand skills with motivated students, passionate teachers, and a hands-on curriculum teaching you how to build software and solve problems.<br></p>\n<!-- /wp:amp/advancedheading -->\n\n<!-- wp:amp/rowlayout {\"uniqueID\":\"_cf0\",\"tabletLayout\":\"row\",\"collapseGutter\":\"n\",\"colLayout\":\"equal\",\"currentTab\":\"mobile\",\"topPadding\":13,\"bottomPadding\":14,\"leftPadding\":0,\"rightPadding\":0,\"verticalAlignment\":\"middle\"} -->\n<div class=\"wp-block-amp-rowlayout cr alignnone\"><div id=\"ri_cf0\" class=\"ab  ri_cf0\"><div class=\"cw ab2 gd gvn rvmiddle rlequal tlrow rmc-left-to-right mrow\"><!-- wp:amp/column {\"uniqueID\":\"_76e\"} -->\n<div class=\"wp-block-amp-column c c-1 cd_76e\"><div class=\"ci\"><!-- w