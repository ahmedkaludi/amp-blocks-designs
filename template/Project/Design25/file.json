  "content": "<!-- wp:amp/rowlayout {\"uniqueID\":\"_3da\",\"columns\":1,\"colLayout\":\"equal\",\"maxWidth\":1020,\"topPadding\":80,\"bottomPadding\":66,\"bgColor\":\"#fcf5e8\",\"blockAlignment\":\"full\"} -->\n<div class=\"wp-block-amp-rowlayout cr alignfull\"><div id=\"ri_3da\" class=\"ab rb ri_3da\"><div class=\"cw ab1 gd gvd rvtop rlequal tlinherit rmc-left-to-right mrow\"><!-- wp:amp/column {\"uniqueID\":\"_be3\"} -->\n<div class=\"wp-block-amp-column c c-1 cd_be3\"><div class=\"ci\"><!-- wp:ampblocks/heading {\"level\":3,\"uniqueID\":\"_ac2\",\"color\":\"#000000\",\"size\":40,\"typography\":\"Work Sans\",\"googleFont\":true,\"fontSubset\":\"latin\",\"fontVariant\":\"700\",\"fontWeight\":\"700\",\"bottomMargin\":0} -->\n<h3 id=\"h_ac2\" class=\"h_ac2 hp wp-block-amp-advancedheading\" style=\"color:#000000;margin-top:10px;margin-bottom:0px\">A&nbsp;<em>life-changing</em>  experience</h3>\n<!-- /wp:ampblocks/heading -->\n\n<!-- wp:amp/advancedheading {\"uniqueID\":\"_965\",\"size\":17,\"lineHeight\":28,\"typography\":\"Work Sans\",\"googleFont\":true,\"fontSubset\":\"latin\",\"fontVariant\":\"regular\",\"fontWeight\":\"400\",\"topMargin\":25} -->\n<p id=\"h_965\" class=\"h_965 pp wp-block-amp-advancedheading\" style=\"margin-top:25px;margin-bottom:10px\">During our 9-week (full-time) or 24-week (part-time) coding Bootcamp, learn the most in-demand skills with motivated students, passionate teachers, and a hands-on curriculum teaching you how to build software and solve problems.<br></p>\n<!-- /wp:amp/advancedheading -->\n\n<!-- wp:amp/rowlayout {\"uniqueID\":\"_1f0\",\"tabletLayout\":\"row\",\"collapseGutter\":\"n\",\"colLayout\":\"equal\",\"currentTab\":\"mobile\",\"topPadding\":13,\"bottomPadding\":14,\"leftPadding\":0,\"rightPadding\":0,\"verticalAlignment\":\"middle\"} -->\n<div class=\"wp-block-amp-rowlayout cr alignnone\"><div id=\"ri_1f0\" class=\"ab  ri_1f0\"><div class=\"cw ab2 gd gvn rvmiddle rlequal tlrow rmc-left-to-right mrow\"><!-- wp:amp/column {\